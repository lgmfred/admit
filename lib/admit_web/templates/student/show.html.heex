<h1>Show Student</h1>

<ul>

  <li>
    <strong>ID:</strong>
    <%= @student.id %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @student.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @student.email %>
  </li>

  <li>
    <strong>Birth date:</strong>
    <%= @student.birth_date %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.student_path(@conn, :edit, @student) %></span> |
<span><%= link "Back", to: Routes.student_path(@conn, :index) %></span>
